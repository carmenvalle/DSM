<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="DiagGen.Infraestructure"
		                   namespace="DiagGen.Infraestructure.EN.Diag">
		    
		    
			
				<class name="FavoritoNH" table="Favorito"> 
				  	
						<id name="IdFavorito" column="idFavorito" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
			
			
			
		  			
						<many-to-one name="Usuario" class="DiagGen.Infraestructure.EN.Diag.UsuarioNH" >	
								<column name="FK_idUsuario_idUsuario"/> 
						</many-to-one>
											 					
			
		  			
						<many-to-one name="Producto" class="DiagGen.Infraestructure.EN.Diag.ProductoNH" >	
								<column name="FK_idProducto_idProducto"/> 
						</many-to-one>
											 					
					
					
					
		
		 </class>	
		  
		  <query name="FavoritoNHReadAllHQL">from FavoritoNH</query>
		</hibernate-mapping>
	