<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="DiagGen.Infraestructure"
		                   namespace="DiagGen.Infraestructure.EN.Diag">
		    
		    
			
				<class name="UsuarioNH" table="Usuario"> 
				  	
						<id name="IdUsuario" column="idUsuario" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Nombre" type="string"> 
						<column name="nombre"  not-null="true"/>
					</property>
					<property name="Correo" type="string"> 
						<column name="correo"  not-null="true"/>
					</property>
					<property name="Direccion" type="string"> 
						<column name="direccion"  not-null="true"/>
					</property>
					<property name="Pass" type="string"> 
						<column name="pass"  not-null="true"/>
					</property>
			
			
			
			
			
			
		  			
						<many-to-one name="Rol" class="DiagGen.Infraestructure.EN.Diag.RolNH" >	
								<column name="FK_idRlol_idRol"/> 
						</many-to-one>
											 					
			
		  			
						<bag name="Pedidos" >
							<key>
							
								
								<column name="FK_idUsuario_idUsuario"/>
					      	</key>
							<one-to-many class="DiagGen.Infraestructure.EN.Diag.PedidoNH"/>
					    </bag>
					
			
		  			
						<bag name="Favoritos" >
							<key>
							
								
								<column name="FK_idUsuario_idUsuario"/>
					      	</key>
							<one-to-many class="DiagGen.Infraestructure.EN.Diag.FavoritoNH"/>
					    </bag>
					
			
		  			
						<bag name="DireccionesEnvio" >
							<key>
							
								
								<column name="FK_idUsuario_idUsuario"/>
					      	</key>
							<one-to-many class="DiagGen.Infraestructure.EN.Diag.DireccionEnvioNH"/>
					    </bag>
					
			
		  			
						<bag name="MetodosPago" >
							<key>
							
								
								<column name="FK_idUsuario_idUsuario"/>
					      	</key>
							<one-to-many class="DiagGen.Infraestructure.EN.Diag.MetodoPagoNH"/>
					    </bag>
					
			
		  			
						<bag name="Valoraciones" >
							<key>
							
								
								<column name="FK_idUsuario_idUsuario"/>
					      	</key>
							<one-to-many class="DiagGen.Infraestructure.EN.Diag.ValoracionNH"/>
					    </bag>
					
			
					
					
					
					
					
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="UsuarioNHReadAllHQL">from UsuarioNH</query>
		  <query name="UsuarioNHcerrarSesionHQL"><![CDATA[FROM UsuarioNH]]></query>
		</hibernate-mapping>
	